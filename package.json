{
  "name": "image-size-stream",
  "version": "0.1.1",
  "description": "Detect the width and height of an image in a stream",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/image-size-stream/blob/master/LICENSE"
    }
  ],
  "files": [
    "index.js",
    "LICENSE"
  ],
  "scripts": {
    "pretest": "jscs *.js test/test.js && eslint *.js test/test.js",
    "test": "_mocha --timeout 5000",
    "test-dot": "_mocha --reporter dot --timeout 5000",
    "coverage": "istanbul cover --no-default-excludes -x=node_modules/** _mocha -- -t 5000",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "keywords": [
    "image",
    "size",
    "dimension",
    "detect",
    "width",
    "height",
    "stream",
    "bmp",
    "gif",
    "jpg",
    "jpeg",
    "png",
    "psd",
    "tiff",
    "webp"
  ],
  "dependencies": {
    "image-size": "^0.3.1",
    "through": "^2.3.4"
  },
  "devDependencies": {
    "eslint": "^0.10.2",
    "istanbul": "^0.3.5",
    "istanbul-coveralls": "^1.0.1",
    "jscs": "^1.9.0",
    "mocha": "^2.1.0",
    "run-parallel": "^1.0.0"
  },
  "jscsConfig": {
    "preset": "google",
    "maximumLineLength": 98,
    "requireBlocksOnNewline": true,
    "validateLineBreaks": "LF"
  }
}
